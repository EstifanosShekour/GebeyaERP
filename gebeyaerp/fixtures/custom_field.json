[
  {
    "doctype": "Custom Field",
    "name": "Item-custom_retail_category",
    "dt": "Item",
    "label": "Retail Category",
    "fieldname": "custom_retail_category",
    "fieldtype": "Select",
    "options": "\nFood\nElectronics\nClothing\nHousehold\nMedicine\nOther",
    "insert_after": "item_name",
    "in_list_view": 1,
    "module": "Gebeyaerp"
  },
  {
    "doctype": "Custom Field",
    "name": "Item-custom_reorder_point",
    "dt": "Item",
    "label": "Reorder Point",
    "fieldname": "custom_reorder_point",
    "fieldtype": "Int",
    "insert_after": "custom_retail_category",
